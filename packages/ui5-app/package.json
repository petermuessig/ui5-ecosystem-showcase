{
  "name": "ui5-app",
  "version": "1.0.0",
  "description": "A Simple UI5 application",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "mkdirp dist",
    "build": "ui5 build self-contained",
    "start": "ui5 serve --config ui5-dist.yaml --port 1081",
    "dev": "ui5 serve --port 1081",
    "debug": "node --inspect node_modules/.bin/ui5 serve --port 1081",
    "watch:build": "npm-watch build",
    "watch": "npm-run-all prepare --parallel watch:build start"
  },
  "watch": {
    "build": {
      "patterns": [
        "webapp"
      ],
      "extensions": "html,js,json,xml,properties",
      "quiet": false
    }
  },
  "dependencies": {
    "@openui5/sap.m": "^1.69.1",
    "@openui5/sap.ui.core": "^1.69.1",
    "@openui5/sap.ui.layout": "^1.69.1",
    "@openui5/themelib_sap_fiori_3": "^1.69.1"
  },
  "devDependencies": {
    "@ui5/cli": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "ui5-middleware-cfdestination": "^1.0.0",
    "ui5-middleware-livereload": "^1.0.0",
    "ui5-middleware-livetranspile": "^1.0.0",
    "ui5-middleware-proxy": "^1.0.0",
    "ui5-task-transpile": "^1.0.0",
    "mkdirp": "~0.5.1",
    "rimraf": "~3.0.0"
  },
  "ui5": {
    "dependencies": [
      "@openui5/sap.ui.core",
      "@openui5/sap.m",
      "@openui5/sap.ui.layout",
      "@openui5/themelib_sap_fiori_3",
      "ui5-middleware-cfdestination",
      "ui5-middleware-livereload",
      "ui5-middleware-livetranspile",
      "ui5-middleware-proxy",
      "ui5-task-transpile"
    ]
  }
}
